{
  "css.customData": [".vscode/tailwind.json"],
  "css.validate": true,
  "scss.validate": true,
  "less.validate": true,
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "tailwindCSS.includeLanguages": {
    "typescript": "javascript",
    "typescriptreact": "javascript"
  }
}
